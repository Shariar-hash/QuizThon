{
  "name": "quizthon",
  "version": "1.0.0",
  "description": "Interactive quiz platform for testing knowledge across multiple categories",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "live-server --port=3001 --entry-file=index.html",
    "build": "npm install --production",
    "prestart": "npm run build"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "live-server": "^1.2.2"
  },
  "keywords": ["quiz", "trivia", "education", "learning", "knowledge", "interactive", "web-app"],
  "author": "QuizThon Team",
  "license": "MIT"
}
